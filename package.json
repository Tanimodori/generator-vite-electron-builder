{
  "name": "generator-vite-electron-builder",
  "version": "0.0.1",
  "description": "A scaffolding tool to create project from vite-electron-builder template",
  "scripts": {
    "build": "vite build",
    "test": "npm run build && npm run test:run",
    "test:run": "npm run test:e2e && npm run test:unit",
    "test:unit": "npx ts-mocha test/dist",
    "test:e2e": "yo ./generator/app"
  },
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.2",
    "@types/validate-npm-package-name": "^4.0.0",
    "@types/yeoman-generator": "^5.2.10",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.0.0",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.3",
    "yeoman-generator": "^5.6.1",
    "yeoman-test": "^6.3.0",
    "yo": "^4.3.0"
  },
  "files": [
    "generator"
  ],
  "main": "./generator/app/index.js",
  "exports": "./generator",
  "author": "Tanimodori",
  "license": "MIT",
  "keywords": [
    "yeoman-generator",
    "scaffold"
  ],
  "dependencies": {
    "validate-npm-package-name": "^4.0.0"
  }
