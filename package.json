{
  "name": "generator-vite-electron-builder",
  "version": "0.0.1",
  "description": "A scaffolding tool to create project from vite-electron-builder template",
  "scripts": {
    "build": "vite build",
    "test": "node dist/index.mjs"
  },
  "devDependencies": {
    "@types/node": "^18.6.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.3"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "author": "Tanimodori",
  "license": "MIT",
  "keywords": [
    "yeoman-generator",
    "scaffold"
  ],
  "dependencies": {
    "yeoman-generator": "^5.6.1"
  }
